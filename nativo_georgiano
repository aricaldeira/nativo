    key <AB06> { [ Georgian_jhan ] };
    key <AB07> { [ Georgian_vin ] };
    key <AB08> { [ Georgian_gan ] };
    key <AB09> { [ Georgian_fi ] };
    key <AB10> { [ Georgian_zen ] };

};

