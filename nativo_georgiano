//#define XK_Georgian_hoe               U10f5  /* U+10F5 GEORGIAN LETTER HOE */

    //
    // Linha superior
    //
    key <AD04> { [ Georgian_hae, Georgian_xan ] };
    key <AD05> { [ Georgian_shin ] };
    key <AD06> { [ Georgian_qar ] };
    key <AD07> { [ Georgian_las ] };
    key <AD08> { [ Georgian_tan, Georgian_tar ] };
    key <AD09> { [ Georgian_khar, Georgian_kan ] };
    key <AD10> { [ Georgian_phar, Georgian_par ] };

    //
    // Linha central
    //
    key <AC01> { [ Georgian_in ] };
    key <AC02> { [ Georgian_en ] };
    key <AC03> { [ Georgian_an ] };
    key <AC04> { [ Georgian_on ] };
    key <AC05> { [ Georgian_un ] };
    key <AC06> { [ Georgian_man ] };
    key <AC07> { [ Georgian_don ] };
    key <AC08> { [ Georgian_san ] };
    key <AC09> { [ Georgian_rae, Georgian_ghan ] };
    key <AC10> { [ Georgian_nar ] };

    //
    // Linha inferior
    //
    key <AB01> { [ Georgian_jil ] };
    key <AB02> { [ Georgian_chin, Georgian_char ] };
    key <AB03> { [ Georgian_zhar ] };
    key <AB04> { [ Georgian_ban ] };
