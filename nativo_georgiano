partial alphanumeric_keys
xkb_symbols "nativo_georgiano" {

//#define XK_Georgian_he                U10f1  /* U+10F1 GEORGIAN LETTER HE */
//#define XK_Georgian_hie               U10f2  /* U+10F2 GEORGIAN LETTER HIE */
//#define XK_Georgian_we                U10f3  /* U+10F3 GEORGIAN LETTER WE */
