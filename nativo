//
// Leiautes Nativo
//

default
partial alphanumeric_keys
xkb_symbols "nativo" {
    include "nativo_base(nativo_letras)"
    include "nativo_base(nativo_simbolos)"
    include "nativo_base(nativo_base)"
};

partial alphanumeric_keys
xkb_symbols "nativo_acento_combinante" {
    include "nativo(nativo)"
    include "nativo_base(nativo_simbolos_acento_combinante)"
};

