//
// ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━━━┓
// │     │ 1   │ 2   │ 3   │ 4   │ 5   │ 6   │ 7   │ 8   │ 9   │ 0   │     │     ┃Backspace  ┃
// │     │     │     │     │     │     │     │     │     │     │     │     │     ┃           ┃
// ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━━━┫
//
partial keypad_keys
xkb_symbols "numeros_um_a_zero" {
    //
    // Linha numérica
    //
    // O uso do tipo EIGHT_LEVEL_SEMIALPHABETIC faz com que, pra essas teclas, o CapsLock
    // tenha o mesmo efeito do NumLock, ou seja, CapsLock vai fazer com que os números
    // sejam o padrão
    //
    key <TLDE> { type="EIGHT_LEVEL"               , [ NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE01> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        1, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE02> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        2, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE03> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        3, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE04> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        4, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE05> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        5, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE06> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        6, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE07> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        7, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE08> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        8, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE09> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        9, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE10> { type="EIGHT_LEVEL_SEMIALPHABETIC", [ NoSymbol,        0, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE11> { type="EIGHT_LEVEL"               , [ NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
    key <AE12> { type="EIGHT_LEVEL"               , [ NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol, NoSymbol ] };
