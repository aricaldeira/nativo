    key <AD10> { type = "EIGHT_LEVEL_ALPHABETIC", [       Cyrillic_pe,       Cyrillic_PE ] };

    //
    // Linha central
    //
    key <AC01> { type = "EIGHT_LEVEL_ALPHABETIC", [        Cyrillic_i,        Cyrillic_I ] };
    key <AC02> { type = "EIGHT_LEVEL_ALPHABETIC", [       Cyrillic_ie,       Cyrillic_IE ] };
