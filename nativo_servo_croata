// ┃       ┃     │     │     │ H   │ Š   │ W   │ L   │ T ‘ │ K   │ P “ │     │     ┃Enter    ┃
// ┃Tab    ┃     │     │     │     │   Ś │   X │   ‚ │   ’ │   „ │   ” │     │     ┃         ┃
// ┣━━━━━━━┻━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┓       ┃
// ┃Caps     ┃ I   │ E   │ A   │ O   │ U   │ M   │ D   │ S   │ R   │ N   │     │     ┃       ┃
// ┃Lock     ┃     │     │     │     │     │     │   › │   ‹ │   » │   « │     │     ┃       ┃
// ┣━━━━━┳━━━┹─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┲━━━┻━━━━━━━┫
// ┃     ┃ Ć   │ J   │ Č   │ Ž   │ B   │ C   │ Đ   │ V   │ G   │ F   │ Z   │     ┃           ┃
// ┃Shift┃     │   Y │   Ć │   Ź │     │     │   Q │     │     │     │     │     ┃Shift      ┃
// ┣━━━━━┻┳━━━━┷━┳━━━┷━━━┱─┴─────┴─────┴─────┴─────┴─────┴─────┴─┲━━━┷━━┳━━┷━━━┳━┻━━━━┳━━━━━━┫
// ┃      ┃      ┃       ┃                                       ┃      ┃      ┃      ┃      ┃
// ┃Ctrl  ┃Meta  ┃Alt    ┃                                       ┃AltGr ┃Meta  ┃Menu  ┃Ctrl  ┃
// ┗━━━━━━┻━━━━━━┻━━━━━━━┹───────────────────────────────────────┺━━━━━━┻━━━━━━┻━━━━━━┻━━━━━━┛
//
partial alphanumeric_keys
xkb_symbols "nativo_servo_croata_latim" {
    include "nativo_moeda(nativo_moeda_euro)"
    include "nativo_aspas(nativo_aspas_polones)"

    //
    // Linha superior
    //
    key <AD05> { type="EIGHT_LEVEL_ALPHABETIC", [ scaron, Scaron, sacute, Sacute, x, X ] };
    key <AD06> { type="EIGHT_LEVEL_ALPHABETIC", [ NoSymbol, NoSymbol, x, X ] };
    key <AD09> { type="EIGHT_LEVEL_ALPHABETIC", [ k, K, NoSymbol, NoSymbol, c, C ] };

    //
    // Linha inferior
    //
    key <LSGT> { type="EIGHT_LEVEL_ALPHABETIC", [ cacute, Cacute ] };
    key <AB01> { type="EIGHT_LEVEL_ALPHABETIC", [ j, J, y, Y ] };
    key <AB02> { type="EIGHT_LEVEL_ALPHABETIC", [ ccaron, Ccaron, cacute, Cacute, ccedilla, Ccedilla ] };
    key <AB03> { type="EIGHT_LEVEL_ALPHABETIC", [ zcaron, Zcaron, zacute, Zacute ] };
    key <AB05> { type="EIGHT_LEVEL_ALPHABETIC", [ c, C, NoSymbol, NoSymbol, k, K ] };
    key <AB06> { type="EIGHT_LEVEL_ALPHABETIC", [ dstroke, Dstroke, q, Q, q, Q ] };
};


